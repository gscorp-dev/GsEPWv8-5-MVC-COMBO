<script>

        $('#chkSelectAll').click(function (event) {
            if ($(this).is(":checked")) {
                $('.checkbox').each(function () {
                    $(this).prop("checked", true);
                });
            }
            else {
                $('.checkbox').each(function () {
                    $(this).prop("checked", false);
                });
            }
        });


        $("input[name=chkAll").click(function () {
            if ($(this).prop("checked") == true) {

                $("input[name='tblcheckboxUpdateDim']").prop("checked", true);
            }
            else if ($(this).prop("checked") == false) {

                $("input[name='tblcheckboxUpdateDim']").prop("checked", false);
            }

        });



        $("input[name='tblcheckboxUpdateDim']").click(function () {

            if ($("input[name='tblcheckboxUpdateDim']").length == $("input[name='tblcheckboxUpdateDim']:checked").length) {
                $("#chkAll").prop("checked", true);
            }
            else {
                $("#chkAll").prop("checked", false);
            }

        });
        $("input[name='tblcheckbox']").on('change', function () {
            $("input[name='tblcheckboxUpdateDim']").not(this).prop('checked', false);
        });


</script>



