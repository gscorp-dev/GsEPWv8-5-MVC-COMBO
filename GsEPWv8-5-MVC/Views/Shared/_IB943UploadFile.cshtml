@model GsEPWv8_5_MVC.Model.IB943UploadFileModel


@{
    var Ordercount = Model.ListIB943UploadFileDtl.Count;
    var l_int_error_count = Model.ListIB943InvalidData.Count;
    var l_str_error_desc = Model.error_desc;
    Int32 l_dbl_total_ctns = Model.ListIB943UploadFileDtl.Sum(item => item.ordr_ctn);
    decimal l_dbl_total_cube = Model.ListIB943UploadFileDtl.Sum(item => (item.ctn_cube * item.ordr_ctn));
    decimal l_dbl_total_weight = Model.ListIB943UploadFileDtl.Sum(item => (item.ctn_wgt * item.ordr_ctn));
}
<style>

</style>
<div id="IB943showgrid" class="box-header" style="padding: 5px; background-color: #eee;border-radius: 5px;">

    <div class="gridSummary">
        <table style="width:100%">
            <tr>
                <td style="width:12%;text-align:right">
                    <span style="font-size: 1.5em;color:#116b42;font-weight: bold;">
                        Total Records:
                    </span>
                    <label class="totalCount" style="text-align:left">
                        @Ordercount
                    </label>
                </td>
                <td style="width:12%;text-align:right">
                <span style="font-size: 1.5em;color:#116b42;font-weight: bold;">
                    Total Ctn(s):
                </span>
                <label class="totalCount" style="text-align:left">
                    @l_dbl_total_ctns
                </label>
                </td>
                <td style="width:12%;text-align:right">

                    <span style="font-size: 1.5em;color:#116b42;font-weight: bold;">
                        Total Cube:
                    </span>
                    <label class="totalCount" style="text-align:left">
                        @l_dbl_total_cube
                    </label>

                </td>
                <td style="width:12%;text-align:right">

                    <span style="font-size: 1.5em;color:#116b42;font-weight: bold;">
                        Total Weight:
                    </span>
                    <label class="totalCount" style="text-align:left">
                        @l_dbl_total_weight
                    </label>
                  <td>

                    </td>
                
                <td style="width:52%;text-align:center">
                    <span style="font-size: 1.5em;color:red;font-weight: bold;">
                       
                    </span>
                    <label class="totalCount" style="text-align:left;color:red;">
                        @ViewBag.strDuplicateCntrList
                    </label>
                </td>

            </tr>
        </table>
    </div>
    <div>

        <table id="GridIB943Main" class="table table-bordered table-striped" style="border:1px solid #121213 ;width:100%">
            <thead class="grid_backcolor">
                <tr>
                    <th class="grid_header_backcolor" style="width:3%">Ln#</th>
                    <th class="grid_header_backcolor" style="width:7%">Container ID</th>
                    <th class="grid_header_backcolor" style="width:5%">Style</th>
                    <th class="grid_header_backcolor" style="width:5%">color </th>
                    <th class="grid_header_backcolor" style="width:5%">Size</th>
                    <th class="grid_header_backcolor" style="width:10%">Desc</th>
                    <th class="grid_header_backcolor" style="width:4%">Item Qty</th>
                    <th class="grid_header_backcolor" style="width:4%">Ctns</th>
                    <th class="grid_header_backcolor" style="width:3%">PPK</th>
                    <th class="grid_header_backcolor" style="width:3%">Length</th>
                    <th class="grid_header_backcolor" style="width:3%">Width</th>
                    <th class="grid_header_backcolor" style="width:3%">Height</th>
                    <th class="grid_header_backcolor" style="width:4%">Cube</th>
                    <th class="grid_header_backcolor" style="width:4%">Weight</th>
                    <th class="grid_header_backcolor" style="width:5%;display:none;">Factory</th>
                    <th class="grid_header_backcolor" style="width:14%">HeaderInfo</th>
                    <th class="grid_header_backcolor" style="width:5%">VendorPO</th>
                    <th class="grid_header_backcolor" style="width:5%;display:none;">Customer</th>
                    <th class="grid_header_backcolor" style="width:5%">CustPO</th>
                    <th class="grid_header_backcolor" style="width:5%;display:none;">Pick List</th>

                    <th class="grid_header_backcolor" style="width:10%;display:none;">Dtl Note</th>
               
                </tr>
            </thead>

            <tbody>
                @for (int i = 0; i < Model.ListIB943UploadFileDtl.Count; i++)
                {
                    var wgt = (Model.ListIB943UploadFileDtl[i].ctn_wgt);
                    var cube = (Model.ListIB943UploadFileDtl[i].ctn_cube);
                    var l_dec_wgt = wgt.ToString("F2");
                    var l_dec_cube = cube.ToString("F3");
                    <tr class="row-select">
                        <td style="font-size:11px; color:black; font-weight:bold;text-align:right">@Model.ListIB943UploadFileDtl[i].dtl_line</td>
                        <td style="font-size:11px; color:black; font-weight:bold;">@Model.ListIB943UploadFileDtl[i].cntr_id</td>
                        <td style="font-size:11px; color:black; font-weight:bold;">@Model.ListIB943UploadFileDtl[i].itm_num</td>
                        <td style="font-size:11px; color:black; font-weight:bold; text-align:left"> @Model.ListIB943UploadFileDtl[i].itm_color</td>
                        <td style="font-size:11px; color:black; font-weight:bold; text-align:left">@Model.ListIB943UploadFileDtl[i].itm_size</td>
                        <td style="font-size:11px; color:black; font-weight:bold;">@Model.ListIB943UploadFileDtl[i].itm_name</td>

                        <td style="font-size:11px; color:black; font-weight:bold;text-align:right">@Model.ListIB943UploadFileDtl[i].ordr_qty</td>
                        <td style="font-size:11px; color:black; font-weight:bold;text-align:right">@Model.ListIB943UploadFileDtl[i].ordr_ctn</td>
                        <td style="font-size:11px; color:black; font-weight:bold;text-align:right">@Model.ListIB943UploadFileDtl[i].ctn_qty</td>
                        <td style="font-size:11px; color:black; font-weight:bold; text-align:right">@Model.ListIB943UploadFileDtl[i].ctn_length</td>
                        <td style="font-size:11px; color:black; font-weight:bold;text-align:right">@Model.ListIB943UploadFileDtl[i].ctn_width</td>
                        <td style="font-size:11px; color:black; font-weight:bold; text-align:right">@Model.ListIB943UploadFileDtl[i].ctn_height</td>
                        <td style="font-size:11px; color:black; font-weight:bold;text-align:right">@Model.ListIB943UploadFileDtl[i].ctn_cube</td>
                        <td style="font-size:11px; color:black; font-weight:bold;text-align:right">@Model.ListIB943UploadFileDtl[i].ctn_wgt</td>
                        <td style="font-size:11px; color:black; font-weight:bold; text-align:left;display:none;"> @Model.ListIB943UploadFileDtl[i].factory_id</td>
                        <td style="font-size:11px; color:black; font-weight:bold;">@Model.ListIB943UploadFileDtl[i].header_data</td>
                        <td style="font-size:11px; color:black; font-weight:bold; text-align:left"> @Model.ListIB943UploadFileDtl[i].po_num</td>
                        <td style="font-size:11px; color:black; font-weight:bold; text-align:left;display:none;"> @Model.ListIB943UploadFileDtl[i].cust_name</td>
                        <td style="font-size:11px; color:black; font-weight:bold; text-align:left">@Model.ListIB943UploadFileDtl[i].cust_po_num</td>
                        <td style="font-size:11px; color:black; font-weight:bold; text-align:left;display:none;">@Model.ListIB943UploadFileDtl[i].pick_list</td>
                        <td style="font-size:11px; color:black; font-weight:bold; text-align:left;display:none;">@Model.ListIB943UploadFileDtl[i].dtl_note</td>
                 

                    </tr>
                }
            </tbody>
        </table>

    </div>
    <div id="IB943ErrorGrid" class="box-header" style="padding: 5px; background-color: #eee;border-radius: 5px;">
        <div style="font-size:14px; color:red; font-weight:bold;">
            943 File Upload Error Summary:
            <label id="l_str_error_count" style="text-align:left">
                @l_int_error_count
            </label>
        </div>

        <div>
            <input type="hidden" value="@ViewBag.l_int_error_count" id="l_int_error_count" />
            <input type="hidden" value="@ViewBag.l_str_error_desc" id="l_str_error_desc" />
        </div>

        <br />
        <div>
            <table class="table table-bordered table-striped " style="border:1px solid #121213 ;width:100%" id="GridIB943Error">
                <thead class="grid_backcolor">
                    <tr>
                        <th class="grid_header_backcolor" style="width:10%">Container ID</th>
                        <th class="grid_header_backcolor" style="width:5%">Line#</th>
                        <th class="grid_header_backcolor" style="width:50%">Line Data</th>
                        <th class="grid_header_backcolor" style="width:35%">Error description</th>
                    </tr>
                </thead>

                <tbody>

                    @for (int i = 0; i < Model.ListIB943InvalidData.Count; i++)
                    {


                        <tr style="background-color:white;" class="row-select">
                            <td style="font-size:13px; color:red; font-weight:bold; text-align:right ;width:10%">@Model.ListIB943InvalidData[i].cntr_id</td>
                            <td style="font-size:13px; color:red; font-weight:bold; width:5%">@Model.ListIB943InvalidData[i].line_num</td>

                            <td style="font-size:13px; color:red; font-weight:bold; width:50%">@Model.ListIB943InvalidData[i].line_data</td>
                            <td style="font-size:13px; color:red; font-weight:bold; width:35%">@Model.ListIB943InvalidData[i].error_desc</td>

                        </tr>


                    }

                </tbody>
            </table>
        </div>
    </div>

</div>






