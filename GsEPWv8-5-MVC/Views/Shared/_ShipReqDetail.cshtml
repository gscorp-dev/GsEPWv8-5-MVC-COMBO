            <div class="box-body">
                <div class="box-header">
                    <div class="box">
                        <div id="table-Details">
                            @*<div style="overflow-y:auto; height:auto; width:auto;">*@
                            <table id="GridShipReq" class="table table-bordered table-striped" style="border:1px solid #121213;">
                                <thead class="grid_backcolor">
                                    <tr>
                                        <th class="grid_header_backcolor">Ln</th>
                                        <th class="grid_header_backcolor">Status</th>
                                        <th class="grid_header_backcolor">Step</th>
                                        <th class="grid_header_backcolor">Style</th>
                                        <th class="grid_header_backcolor">Color</th>
                                        <th class="grid_header_backcolor">Size</th>
                                        <th class="grid_header_backcolor">PO#</th>
                                        <th class="grid_header_backcolor">L/W/H</th>
                                        <th class="grid_header_backcolor">UOM</th>
                                        <th class="grid_header_backcolor">Order Qty</th>
                                        <th class="grid_header_backcolor">PPK</th>
                                        <th class="grid_header_backcolor">Ctns</th>
                                    </tr>
                                </thead>
                                <tbody style="background-color:rgba(0, 0, 0, 0.12);">
                                    @for (int i = 0; i < Model.LstOutboundInqpickstyleRpt.Count; i++)
                                    {
                                        var Length = (Model.LstOutboundInqpickstyleRpt[i].Length);
                                        var Width = (Model.LstOutboundInqpickstyleRpt[i].Width);
                                        var Height = (Model.LstOutboundInqpickstyleRpt[i].Height);
                                        var l_dec_Length = Length.ToString("F2");
                                        var l_dec_Width = Width.ToString("F2");
                                        var l_dec_Height = Height.ToString("F2");
                                        var l_str_LWH = l_dec_Length + "/" + l_dec_Width + "/" + l_dec_Height;
                                        <tr style="background-color:white;">
                                            <td style="font-size:10px; color:black; font-weight:bold; text-align:left;">@Model.LstOutboundInqpickstyleRpt[i].LineNum</td>
                                            @if (@Model.LstOutboundInqpickstyleRpt[i].status.Trim() == "O")
                                            {
                                                <td style="font-size:10px; color:black; font-weight:bold;text-align:left;">OPEN</td>
                                            }
                                            else if (@Model.LstOutboundInqpickstyleRpt[i].status.Trim() == "P")
                                            {
                                                <td style="font-size:10px; color:black; font-weight:bold;text-align:left;">POST</td>
                                            }
                                            else if (@Model.LstOutboundInqpickstyleRpt[i].status.Trim() == "A")
                                            {
                                                <td style="font-size:10px; color:black; font-weight:bold;text-align:left;">ALOC</td>
                                            }
                                            else if (@Model.LstOutboundInqpickstyleRpt[i].status.Trim() == "S")
                                            {
                                                <td style="font-size:10px; color:black; font-weight:bold;text-align:left;">SHIP</td>
                                            }
                                            else if (@Model.LstOutboundInqpickstyleRpt[i].status.Trim() == "B")
                                            {
                                                <td style="font-size:10px; color:black; font-weight:bold;text-align:left;">B/O</td>
                                            }
                                            <td style="font-size:10px; color:black; font-weight:bold;text-align:left;">@Model.LstOutboundInqpickstyleRpt[i].STEP</td>
                                            <td style="font-size:10px; color:black; font-weight:bold;text-align:left;">@Model.LstOutboundInqpickstyleRpt[i].Style</td>
                                            <td style="font-size:10px; color:black; font-weight:bold;text-align:left;">@Model.LstOutboundInqpickstyleRpt[i].Color</td>
                                            <td style="font-size:10px; color:black; font-weight:bold;text-align:left;">@Model.LstOutboundInqpickstyleRpt[i].Size</td>
                                            <td style="font-size:10px; color:black; font-weight:bold;text-align:left;">@Model.LstOutboundInqpickstyleRpt[i].po_num</td>
                                            <td style="font-size:10px; color:black; font-weight:bold;text-align:right;">@l_str_LWH</td>
                                            <td style="font-size:10px; color:black; font-weight:bold;text-align:left;">@Model.LstOutboundInqpickstyleRpt[i].QtyUOM</td>
                                            <td style="font-size:10px; color:black; font-weight:bold;text-align:right;">@Model.LstOutboundInqpickstyleRpt[i].OrdQty</td>
                                            <td style="font-size:10px; color:black; font-weight:bold;text-align:right;">@Model.LstOutboundInqpickstyleRpt[i].ppk</td>
                                            <td style="font-size:10px; color:black; font-weight:bold;text-align:right;">@Model.LstOutboundInqpickstyleRpt[i].OrdCtns</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                            @*</div>*@
                        </div>
