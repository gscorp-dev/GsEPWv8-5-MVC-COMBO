<script>
    $("#btnAlocEditSave").click(function () {
        debugger
        var pstrCmpId = $('#ae_cmp_id').val();
        var pstrAlocDocId = $('#ae_aloc_doc_id').val();
        var pstrSrNum = $('#ae_sr_no').val();
        var pstrAlocDocDt = $('#ae_aloc_dt').val();
        var pstrShipDt = $('#ae_ship_dt').val();
        var pstrCancelDt = $('#ae_cancel_dt').val();
        var pstrPriceTkt = $('#ae_price_tkt').val();
        var pstrCustId = $('#ae_cust_id').val();
        var pstrCustName = $('#ae_cust_name').val();
        var pstrCustOrdrNum = $('#ae_cust_ordr_num').val();
        var pstrCustOrdrDt = $('#ae_cust_ordr_dt').val();
        var pstrNote = $('#ae_note').val();


        $("#ajaxLoadingEdit").show();
        $('#btnAlocEditClose').attr('disabled', 'disabled');


        if (pstrAlocDocDt == "") {
            sweetAlert("", "Please Enter Allocation Date ", "");
            $('#ae_aloc_doc_id').focus();
            $("#ajaxLoadingEdit").hide();
            $('body,html').animate({ scrollTop: 0 }, 0);
            er = 1;
            return false;
        }
        else {
            $.ajax({
                type: "GET",
                url: '@Url.Content("~/OBAllocation/SaveOBAlocEditHdr")',
                data: {
                    "pstrCmpId": pstrCmpId,
                    "pstrAlocDocId": pstrAlocDocId,
                    "pstrAlocDocDt": pstrAlocDocDt, "pstrSrNum": pstrSrNum,
                    "pstrShipDt": pstrShipDt, "pstrCancelDt": pstrCancelDt, "pstrPriceTkt": pstrPriceTkt, "pstrCustId": pstrCustId,
                    "pstrCustName": pstrCustName, "pstrCustOrdrNum": pstrCustOrdrNum, "pstrCustOrdrDt": pstrCustOrdrDt, "pstrNote": pstrNote
                },
                dataType: 'html',
                success: function (e) {
                    debugger
