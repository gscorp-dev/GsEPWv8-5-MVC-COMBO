<script type="text/javascript">

    $("#btnAlocEditClose").click(function () {
        var p_str_cmp_id = $("#ae_cmp_id").val();
        var p_str_ShipReq_id = $('#ae_sr_no').val();
        var p_str_AlocdocId = $("#ae_aloc_doc_id").val();

        $("#ajaxLoadingAloc").show();

        debugger
        $('#myModalEdit').modal('hide');
        $.ajax({
            type: "get",
            url: "/OutboundInq/GetOutboundInqDtl",
            data: {
                "p_str_cmp_id": p_str_cmp_id,
                "p_str_so_num": p_str_ShipReq_id,
                "p_str_AlocdocId": p_str_AlocdocId
            },
            datatype: 'html',
            success: function (e) {
                debugger
                $("#ajaxLoadingAloc").hide();
                $('#showgrid').html("");
                $('#showgrid').html(e);
            },
        })

    });

    var urlAlocLineEdit = '/OBAllocation/getAlocCtnEditByLine';
    $(function () {
        $('#grdAlocSmryEdit tbody').on('click', '.linkAlocLineEdit', function () {
            debugger
            $("#ajaxLoadingEdit").show();
            var $StockAdjButtonClicked = $(this);
            var p_str_cmp_id = $StockAdjButtonClicked.attr('aed_str_cmp_id');
            var p_str_aloc_doc_id = $StockAdjButtonClicked.attr('aed_aloc_doc_id');
            var p_str_so_no = $StockAdjButtonClicked.attr('aed_so_no');

            var p_str_line_num = $StockAdjButtonClicked.attr('aed_line_num');
            var p_str_so_line_num = $StockAdjButtonClicked.attr('aed_so_line_num');

            var p_str_itm_code = $StockAdjButtonClicked.attr('c_adj_str_itm_code');
            var p_str_itm_num = $StockAdjButtonClicked.attr('c_adj_str_itm_num');
            var p_str_itm_color = $StockAdjButtonClicked.attr('c_adj_str_itm_color');
            var p_str_itm_size = $StockAdjButtonClicked.attr('c_adj_str_itm_size');
            var p_str_due_qty = $StockAdjButtonClicked.attr('c_adj_str_due_qty');
            debugger

            $.ajax({
                type: "GET",
                url: urlAlocLineEdit,
                contentType: "application/json; charset=utf-8",
                data: {
                    "p_str_cmp_id": p_str_cmp_id, "p_str_aloc_doc_id": p_str_aloc_doc_id, "p_str_so_no": p_str_so_no,
                    "p_str_line_num": p_str_line_num, "p_str_so_line_num": p_str_so_line_num,
                    "p_str_itm_code": p_str_itm_code, "p_str_itm_num": p_str_itm_num,
                    "p_str_itm_color": p_str_itm_color, "p_str_itm_size": p_str_itm_size, "p_str_due_qty": p_str_due_qty
                },
                datatype: "html",
                success: function (e) {
                    $("#ajaxLoadingEdit").hide();
                    $('#divAlocCtnLineEdit').html("");
                    $('#divAlocCtnLineEdit').html(e);
                    $('#divAlocCtnLineEdit').show();
                    $("#childtabsAloc").tabs("option", "active", $("#childtabsAloc").tabs('option', 'active') + 1);
                    $('#divAlocCtnGrid').hide();
                    //$('#divAlocCtnGrid').toggle();
                },
                error: function () {
                    $("#ajaxLoadingEdit").hide();
                    swal("Dynamic content load failed.");
                }
            });
        });
    });
</script>
