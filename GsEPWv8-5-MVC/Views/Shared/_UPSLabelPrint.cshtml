    string Username = Model.UPSGsSecurity.Username;
    string Password = Model.UPSGsSecurity.Password;
    int tot_ctns = 0;
    decimal tot_cude = 0;
    decimal tot_wgt = 0;
    int lintTotLines = Model.OrderDetails.Count;
}

