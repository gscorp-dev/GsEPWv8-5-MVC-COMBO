<table cellpadding="0" id="grdOrderDtl" class="table table-bordered table-striped" style="border:1px solid #121213 ;">
    <thead class="grid_backcolor" style="display:table; width:100%;  table-layout:fixed;border-spacing: 10px;">
        <tr>
            <th class="grid_header_backcolor">Sel</th>
            <th class="grid_header_backcolor">Line#</th>
            <th class="grid_header_backcolor">Style</th>
            <th class="grid_header_backcolor">Color</th>
            <th class="grid_header_backcolor">Size</th>
            <th class="grid_header_backcolor">Item Name</th>
            <th class="grid_header_backcolor">Ctn(s)</th>
            <th class="grid_header_backcolor">PPk</th>
            <th class="grid_header_backcolor">Price</th>
            <th class="grid_header_backcolor">Length</th>
            <th class="grid_header_backcolor">Width</th>
            <th class="grid_header_backcolor">Depth</th>
            <th class="grid_header_backcolor">Cube</th>
            <th class="grid_header_backcolor">Weight</th>
        </tr>
    </thead>
    <tbody style="display:block; max-height:500px; overflow-y:scroll;">
        @for (int i = 0; i < Model.OrderDetails.Count; i++)
        {
            int l_int_cur_row = i + 1;
            tot_ctns = tot_ctns + @Model.OrderDetails[i].ordr_ctns;
            tot_cude = tot_cude + (@Model.OrderDetails[i].cube * @Model.OrderDetails[i].ordr_ctns);
            tot_wgt = tot_wgt + (@Model.OrderDetails[i].wgt * @Model.OrderDetails[i].ordr_ctns);

            <tr class="row-select" style="background-color:white;display:table; width:100%; table-layout:fixed;">
                @if (@Model.OrderDetails[i].line_num > 0)
                {
                    <td style="text-align:center"><input class="checkbox" id="chkOrdrLine_@i" name="chkOrdrLine" disabled="disabled" checked type="checkbox" /></td>
                }
                else
                {
                    <td style="text-align:center"><input class="checkbox" id="chkOrdrLine_@i" disabled="disabled" name="chkOrdrLine" type="checkbox" /></td>
                }
                <td style="font-size:12px; color:black; font-weight:bold;">@Html.TextBoxFor(model => model.OrderDetails[i].line_num, new { @id = "ordr_line_num_" + i, @disabled = "true", @style = "width:100px;text-align:left;background-color:white;" })</td>
                <td style="font-size:12px; color:black; font-weight:bold;">@Html.TextBoxFor(model => model.OrderDetails[i].itm_num, new { @id = "ordr_itm_num_" + i, @disabled = "true", @style = "width:100px;text-align:left;background-color:white;" })</td>
                <td style="font-size:12px; color:black; font-weight:bold;"> @Html.TextBoxFor(model => model.OrderDetails[i].itm_color, new { @id = "ordr_itm_color_" + i, @disabled = "true", @style = "width:80px;text-align:left;background-color:white;" })</td>
                <td style="font-size:12px; color:black; font-weight:bold;"> @Html.TextBoxFor(model => model.OrderDetails[i].itm_size, new { @id = "ordr_itm_size_" + i, @disabled = "true", @style = "width:60px;text-align:left;background-color:white;" })</td>
                <td style="font-size:12px; color:black; font-weight:bold;">@Html.TextBoxFor(model => model.OrderDetails[i].itm_name, new { @id = "ordr_itm_name_" + i, @disabled = "true", @style = "width:120px;text-align:left;background-color:white;" })</td>
                <td style="font-size:12px; color:black; font-weight:bold;"> @Html.TextBoxFor(model => model.OrderDetails[i].ordr_ctns, new { @id = "ordr_ordr_ctns_" + i, @style = "width:60px;text-align:left;background-color:lightyellow;" })</td>

                <td style="font-size:12px; color:black; font-weight:bold;  text-align:right">@Html.TextBoxFor(model => model.OrderDetails[i].ctn_qty, new { @id = "ordr_ctn_qty_" + i, @disabled = "true", @style = "width:60px;text-align:right;background-color:white;" })</td>
                <td style="font-size:12px; color:black; font-weight:bold;  text-align:right">@Html.TextBoxFor(model => model.OrderDetails[i].sell_price, new { @id = "ordr_sell_price_" + i, @style = "width:60px;text-align:right;background-color:lightyellow;" })</td>
                <td style="font-size:10px; color:black; font-weight:bold;text-align:right;">@Html.TextBoxFor(model => model.OrderDetails[i].length, new { @id = "ordr_length_" + i, @rowId = @l_int_cur_row, @style = "width:60px;text-align:right;background-color:lightyellow;" })</td>
                <td style="font-size:10px; color:black; font-weight:bold;text-align:right;">@Html.TextBoxFor(model => model.OrderDetails[i].width, new { @id = "ordr_width_" + i, @rowId = @l_int_cur_row, @style = "width:60px;text-align:right;background-color:lightyellow;" })</td>

                <td style="font-size:10px; color:black; font-weight:bold;text-align:right;">@Html.TextBoxFor(model => model.OrderDetails[i].depth, new { @class = "AlocCtns numericOnly", @id = "ordr_depth_" + i, @style = "width:60px;text-align:right;background-color:lightyellow;", @rowId = @l_int_cur_row })</td>
                <td style="font-size:10px; color:black; font-weight:bold;text-align:right;">@Html.TextBoxFor(model => model.OrderDetails[i].cube, new { @class = "AlocQty numericOnly", @id = "ordr_cube_" + i, @style = "width:60px;text-align:right;background-color:lightyellow;", @rowId = @l_int_cur_row })</td>
                <td style="font-size:12px; color:black; font-weight:bold;  text-align:right">@Html.TextBoxFor(model => model.OrderDetails[i].wgt, new { @id = "ordr_wgt_" + i, @style = "width:60px;text-align:right;background-color:lightyellow;" })</td>

            </tr>
        }

    </tbody>

    <tfoot class="grid_backcolor" style="display:table; width:100%;  table-layout:fixed;border-spacing: 10px;">
        <tr>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td style="font-size:14px; color:black; font-weight:bold;  text-align:right">Summary</td>
