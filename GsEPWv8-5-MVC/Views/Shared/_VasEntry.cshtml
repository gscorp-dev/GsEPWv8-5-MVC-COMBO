                qty = 0;
            }
            var price = $("#price_" + i).val();
            if (price == "") {
                price = 0;
            }
            var l_int_qty = $("#qty_" + i).val();
            var l_dec_price = $("#price_" + i).val();
            var l_dec_Amount = $("#amt_" + i).val();
            var l_str_itmnum = $("#itmnum_" + i).val();
            var l_str_note = $("#Note_" + i).val();
            var l_str_itnname = $("#itm_name_" + i).val();
            var l_str_priceuom = $("#price_uom_" + i).val();

            if (p_str_check_value == "true" || p_str_check_value == "") {

                $("#check_" + Minus).prop('checked', true);
                $("#GridID_" + Minus).css("background-color", "#5ae77e");
               
            }
            else {
                $("#check_" + Minus).prop('checked', false);
                $("#GridID_" + Minus).css("background-color", "white");
                
            }
        }
    });


    $('#ship_dt').datetimepicker({
        timepicker: false,
        step: 15, format: 'm-d-Y'
    });
    $('#cust_po_dt').datetimepicker({
        timepicker: false,
        step: 15, format: 'm-d-Y'
    });
    function closeModal() {

        $.ajax({
            url: '@Url.Action("TruncateVasEntry", "VasInquiry")',
            type: 'GET',
            cache: false,
            success: function (data) {
                if (data > 0) {
                }
            }
        });
