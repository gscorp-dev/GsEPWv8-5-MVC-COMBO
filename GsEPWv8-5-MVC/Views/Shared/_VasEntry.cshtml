                                    var p_str_cmp_id = $('#new_cmp_id').val();
                                    var p_str_vas_id = $('#vas_id').val();                //CR - 3PL-MVC-VAS-20180505 Added by Soniya

                                    $('#myModalAdd').modal('hide');
                                    $.ajax({
                                        type: "get",
                                        url: "/VasInquiry/VasInquiryDtl",
                                        data: {
                                            "p_str_cmp_id": p_str_cmp_id,
                                            "p_str_vas_id": p_str_vas_id,
                                        },
                                        datatype: 'html',
                                        success: function (e) {
                                            debugger
                                            $("#ajaxloading").hide();
                                            $('#showgrid').html("");
                                            $('#showgrid').html(e);
                                            $('#btnvas_Close').removeAttr('disabled');
                                        },
                                    })
                                }
                            });
                        });
                    }


                },

            })
        }
    });
