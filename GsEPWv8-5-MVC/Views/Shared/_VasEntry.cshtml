            swal("", "No Item Added.", "");
            return false;
        }
        var ItemDetails = new Array();


        debugger;
        var VasdtlCount = $("#Count_Values").val();
        debugger
        for (var i = 0; i < VasdtlCount; i++) {
            debugger
            var l_int_qty = $("#qty_" + i).val();
            var l_dec_price = $("#price_" + i).val();
            var l_dec_Amount = $("#amt_" + i).val();
            var l_str_itmnum = $("#itmnum_" + i).val();
