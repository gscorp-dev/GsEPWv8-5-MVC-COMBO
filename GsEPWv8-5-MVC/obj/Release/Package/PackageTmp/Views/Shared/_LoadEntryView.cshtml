                                                showCancelButton: false,
                                                confirmButtonClass: "btn-danger",
                                                confirmButtonText: "Ok",
                                                closeOnConfirm: true
                                            },

                                            function () {
                                                debugger
                                                $("#ajaxLoadingLoadEntry").hide();
                                                $('#ModalclsLoadEntry').modal('hide');
                                                $('#ModalLoadEntry').modal('hide');

                                            });

                                        }

                                    },
                                    error: function () {
                                        alert("Dynamic content load failed.");
                                        $("#ajaxLoadingLoadEntry").hide();
                                    }
