<script>
    $("#btnDelCmp").click(function () {
        debugger
        $("#ajaxloading").show();
        var p_str_cmp_id = $("#d_cmb_cmp_id").val();
        sweetAlert({
            title: "",
            text: "Do you want to delete?",
            type: "",
            showCancelButton: true,
            confirmButtonClass: "btn-danger",
            confirmButtonText: "No",
            cancelButtonText: "Yes",
            closeOnConfirm: true,
            closeOnCancel: true
        },
               function (isConfirm) {
                   if (isConfirm) {

                   }

                   else {

                       $.ajax({
                           type: "GET",
                           url: '@Url.Content("~/CompanyMaster/DeleteCmpDtls")',
                           data: {
                               "p_str_cmp_id": p_str_cmp_id,
                           },
                           dataType: 'html',
                           success: function (e) {               
                               if (e > 0) {
                                   setTimeout(function () {
                                       swal({
                                           title: "",
                                           text: "CompId " + p_str_cmp_id + " Deleted Successfully.",
                                           type: "",
                                           showCancelButton: false,
                                           confirmButtonClass: "btn-danger",
                                           confirmButtonText: "Ok",
                                           closeOnConfirm: true
                                       },
                                   function () {
                                       debugger
                                       setTimeout(function () {
                                           debugger
                                           var url = '@Url.Action("CompanyMaster", "CompanyMaster")';
                                           url += '?id=' + p_str_cmp_id;
                                           window.location.href = url;

                                       });
                                   });
                                   }, 1000);
                               }
                               else {
                                   setTimeout(function () {
                                       swal({
                                           title: "",
                                           text: "CompID " + p_str_cmp_id + " In Use.",
                                           type: "",
                                           showCancelButton: false,
                                           confirmButtonClass: "btn-danger",
                                           confirmButtonText: "Ok",
                                           closeOnConfirm: true
                                       },
                                    function () {
                                        debugger
                                        setTimeout(function () {
                                            var p_str_cmp_id = $('#cust_of_cmp_id').val();
                                            $('#CompanyDelete').modal('hide');
                                            $.ajax({
                                                type: "get",
                                                url: "/CompanyMaster/CompanyMasterDetail",
                                                data: {
                                                    "p_str_cmp_id": p_str_cmp_id

                                                },
                                                datatype: 'html',
                                                success: function (e) {
                                                    debugger

                                                    $("#ajaxloading").hide();
                                                    $('#showgrid').html("");
                                                    $('#showgrid').html(e);
                                                },
                                            })

                                        });
                                    });
                                   }, 1000);                                  
                               }

                           },

                       })
                   }
               });
    });
</script>
