<script type="text/javascript">

    $("#btnTrackInclude").click(function () {
        debugger;
        var pstrCmpId = $("#cmp_id").val();
        var pstrTrackNum = $("#a_track_num").val();
        var pstrSoNum = $("#Sonum").val();
        var pstrTrackNumType = $('#a_track_num_type').is(':checked');
        var is_inner_pack = $('#is_inner_pack').is(':checked');
        if (pstrTrackNum == "") {
            swal("", "Please enter the Tracking Number", "");
            return false;
        }

        $("#ajaxLoading").show();
        $.ajax({
            type: "GET",
            url: '@Url.Content("~/OutboundInq/SaveSoTracking")',
            data: {
                "pstrCmpId": pstrCmpId, "pstrSoNum": pstrSoNum, "pstrTrackNum": pstrTrackNum, "pstrTrackNumType": pstrTrackNumType, "pstrMode": "A", "pstrTrackStatus": "OPEN"
            },
            dataType: 'html',
            success: function (e) {
                debugger
                if (e == '1') {
                    sweetAlert("", "Tracking Number Already exists", "");
                    return false;
                }
                else {
                    $("#ajaxLoading").hide();
                    $('#showTrackingEntryGrid').html("");
                    $('#showTrackingEntryGrid').html(e);

                }
            },
        })
    });

    $(function () {
        $("#tabs").tabs();
    });
    $(function () {
        $("#shipreqedittab").tabs();
    });
    $("#btnShipreqEntrySave").click(function () {
        var p_str_cmp_id = $('#cmp_id').val();
        var p_str_vas_id = $('#vas_id').val();
        var p_str_vas_dt = $('#ship_dt').val();
        var p_str_so_num = $('#Vassonum').val();
        var p_str_whs_id = $('#ps_itm_name').val();
        var p_str_ship_to_id = $('#ship_to_id').val();
        var p_str_cust_po_num = $('#cust_po_num').val();
        var p_str_cust_po_dt = $('#cust_po_dt').val();
        var p_str_note = $('#notes').val();
        if (p_str_cmp_id == "") {
            swal("", "Please Select The Company.", "");
            $('#cmp_id').focus();
            $('body,html').animate({ scrollTop: 0 }, 0);
            er = 1;
            return false;
        }
        else {
            $.ajax({
                type: "GET",
                url: '@Url.Content("~/VasInquiry/UpdateShipEntry")',
                data: {
                    "p_str_cmp_id": p_str_cmp_id,
                    "p_str_vas_id": p_str_vas_id,
                    "p_str_vas_dt": p_str_vas_dt,

                    "p_str_so_num": p_str_so_num, "p_str_whs_id": p_str_whs_id, "p_str_ship_to_id": p_str_ship_to_id, "p_str_cust_po_num": p_str_cust_po_num,
                    "p_str_cust_po_dt": p_str_cust_po_dt, "p_str_note": p_str_note
                },
                dataType: 'html',
                success: function (e) {
                    if (e != null) {
                        swal("", "Your record Updated successfully.", "");
                        setTimeout(
                            function RoleCreateMove() {
                                window.location.href = '@Url.Action("VasInquiry", "VasInquiry")';
                            }, 500);

                    }
                },

            })
        }
    });
    $(function () {
        $('#GridDocEntry').DataTable({

            'paging': false,
            'lengthChange': false,
            'searching': true,
            'ordering': false,
            'info': false,
            'autoWidth': false,

        });
        $('#GridDocEntry_paginate').hide();
    })
</script>
