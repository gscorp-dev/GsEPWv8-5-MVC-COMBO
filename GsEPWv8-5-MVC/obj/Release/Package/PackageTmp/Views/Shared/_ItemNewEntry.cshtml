                                                var a_pkg_service = @Model.ListPkgSrvc[j].pkg_service;
                                                var a_pkg_service_exists = @Model.ListPkgSrvc[j].pkg_service_exists;

                                                <tr id="@Model.ListPkgSrvc[j].pkg_service">
                                                    <td id="chkboxPkgService" id1="@a_pkg_service" id2="@a_pkg_service_exists" style="text-align:center">
