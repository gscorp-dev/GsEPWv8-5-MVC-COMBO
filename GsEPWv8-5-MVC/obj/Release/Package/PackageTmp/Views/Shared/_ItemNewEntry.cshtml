                                                <label><b class="Lblmanual">Box Size</b></label>
                                            </td>
                                            <td style="width: 12%; padding-left: 3%;">
                                                @Html.DropDownListFor(model => model.pkg_size, new SelectList(Model.ListPkgBox, "config_name", "config_desc"), new { @id = "a_box_pkg_size", @class = "form-control txtlbl", @style = "width:150px;" })
