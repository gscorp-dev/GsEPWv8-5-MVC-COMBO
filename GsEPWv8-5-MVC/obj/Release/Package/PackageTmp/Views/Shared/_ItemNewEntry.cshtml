                                            <input type="file" name="FileUpload" id="img1" onchange="updateFileName()" style="display:none" title="" />
                                            @Html.TextBoxFor(x => x.objItmImage.img_name, new { @class = "form-control txtlbl", @disabled = "true", @name = "img_name", @id = "img_name" })
                                        </td>
