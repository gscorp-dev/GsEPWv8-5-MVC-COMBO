                                            <tr>
                                                <th class="grid_header_backcolor">Sel</th>
                                                <th class="grid_header_backcolor">Package Service</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @for (int j = 0; j < Model.ListPkgSrvc.Count; j++)
