<style>
    div#ajaxLoadingSaveExpRpt {
        display: none;
        width: 100px;
        height: 100px;
        position: fixed;
        top: 40%;
        left: 53%;
        text-align: center;
        margin-left: -50px;
        margin-top: -100px;
        z-index: 2;
        overflow: auto;
    }
</style>
<style type="text/css">
    fieldset {
        /*margin: 8px;*/
        border: 1px solid silver;
        padding: 8px;
        border-radius: 4px;
    }

    legend {
        padding: 2px;
    }

    legend {
        display: block;
        width: 100%;
        padding: 0;
        margin-bottom: 23px;
        font-size: 14px;
        line-height: inherit;
        color: #e81994;
        border: 0;
        font-weight: bold;
    }

    .fa {
        display: inline-block;
        font: normal normal normal 14px/1 FontAwesome;
        font-size: 12px;
        text-rendering: auto;
        -webkit-font-smoothing: antialiased;
    }

    .ScreenTitle {
        padding-left: 40px;
        font-size: 18px;
        font-family: Verdana;
        font-weight: bold;
        padding-top: 13px;
    }

    .ui-autocomplete {
        color: black;
        font-family: verdana;
        font-size: 12px;
        font-weight: bold;
    }
</style>
@Html.HiddenFor(model => model.action)
@*@Html.HiddenFor(model => model.action)*@
<div class="modal-header">
    @if (Model.action == "1")
    {
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h4 class="modal-title" id="myModalLabel" style="color:white;font-weight:800;text-align:center">ExceptionReport New Configuration </h4>
        <button type="button" id="btnNewExpClose" class="btn-Close" style="float:right;margin-top: -18px;margin-right: 21px;">Close</button>
        <button type="button" id="btnNewExpSave" class="btn-Close" style="float:right;margin-top: -18px;margin-right: 2px;margin-left:-28px;background-color: #00c0ef;">Save</button>

    }
    else if (Model.action == "2")
    {
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h4 class="modal-title" id="myModalLabel" style="color:white;font-weight:800;text-align:center">ExceptionReport Edit Configuration </h4>
        <button type="button" id="btnNewExpEditClose" class="btn-Close" style="float:right;margin-top: -18px;margin-right: 21px;">Close</button>
        <button type="button" id="btnNewExpEdit" class="btn-Close" style="float:right;margin-top: -18px;margin-right: 2px;margin-left:-28px;background-color: #00c0ef;">Save</button>
    }
    else if (Model.action == "3")
    {
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h4 class="modal-title" id="myModalLabel" style="color:white;font-weight:800;text-align:center">ExceptionReport View Configuration </h4>
        <button type="button" id="btnNewViewClose" class="btn-Close" style="float:right;margin-top: -18px;margin-right: 21px;">Close</button>
    }
    else
    {
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h4 class="modal-title" id="myModalLabel" style="color:white;font-weight:800;text-align:center">ExceptionReport Delete Configuration </h4>
        <button type="button" id="btnNewDeleteClose" class="btn-Close" style="float:right;margin-top: -18px;margin-right: 21px;">Close</button>
        <button type="button" id="btnNewDelete" class="btn-Close" style="float:right;margin-top: -18px;margin-right: 2px;margin-left:-28px;background-color: #00c0ef;">Delete</button>
    }
</div>
