<script>
    $('#btnNewExpSave').click(function () {
            debugger
            var p_str_cmp_id = $('#new_cmp_id').val();
            var p_str_rpt_id = $('#NewRptId').val();
            var p_str_mail_to = $('#NewEmailTo').val();
            var p_str_mail_cc = $('#NewEmailCC').val();
            var p_str_content = $('#NewEmailMsg').val();
            var p_str_status = $('#Status').val();
            var p_str_Timehr = $('#Time').val();
            var p_str_Timemin = $('#Timemin').val();
            var p_str_Time = p_str_Timehr + ":" + p_str_Timemin;
            var p_str_action="1";
            var favorite = [];
            $.each($("input[name='SelectedDaysIDSave']:checked"), function () {
                favorite.push($(this).val());
            });
            var p_str_RunningDays = favorite.join(";");
            if (p_str_Timehr == "00")
            {
                sweetAlert("", "Time Required", "");
                return false;
            }
            if (p_str_rpt_id == "") {
                sweetAlert("", "ReportName is Required", "");
                return false;
            }
            if (p_str_mail_to == "") {
                sweetAlert("", "Atleast One Email To is Required", "");
                return false;
            }
            if (p_str_status == "") {
                sweetAlert("", "Status is Required", "");
                return false;
            }
            if (p_str_Timehr == "")
            {
                sweetAlert("", "Hour is Required.", "");
                return false;
            }
            if (p_str_Timemin == "") {
                sweetAlert("", "Minutes is Required.", "");
                return false;
            }
            if (p_str_RunningDays == "") {
                swal("", "Days are Required", "");
                return false;
            }

            $("#ajaxLoadingSaveExpRpt").show();
            $.ajax({
                type: "POST",
                url: '@Url.Content("~/DailyFlashExceptionReport/SaveExceptionReportConfig")',
                data:{
                    "p_str_cmp_id":p_str_cmp_id,"p_str_rpt_id":p_str_rpt_id,"p_str_mail_to":p_str_mail_to,"p_str_mail_cc":p_str_mail_cc,"p_str_content":p_str_content,
                    "p_str_status":p_str_status,"p_str_action":p_str_action,"p_str_RunningDays":p_str_RunningDays,"p_str_Time":p_str_Time
                },

                dataType: "json",

                success: function (e) {
                    if (e == true) {
                        swal({
                            title: "",
                            text: " Details Saved successfully",
                            type: "",
                            showCancelButton: false,
                            confirmButtonClass: "btn-danger",
                            confirmButtonText: "Ok",
                            closeOnConfirm: true
                        },

                       function () {
                           setTimeout(function () {
                               var cmp = $('#new_cmp_id').val();
                            var url = '@Url.Action("DailyFlashExceptionReport", "DailyFlashExceptionReport")';
                               url += '?id=' + cmp;
                            window.location.href = url;
                           }, 10);
                       });

                    }
                    else
                    {
                        sweetAlert("", "Details are already Exist Please Check it", "");
                        $("#ajaxLoadingSaveExpRpt").hide();
                        $('#myModalAddExpRpt').modal('hide');

                    }
                },

                failure: function (response) {
                    alert(response.responseText);
                    $("#ajaxLoadingSaveExpRpt").hide();
                },
                error: function (response) {
                    alert(response.responseText);
                    $("#ajaxLoadingSaveExpRpt").hide();
                }
            });



        })

</script>

