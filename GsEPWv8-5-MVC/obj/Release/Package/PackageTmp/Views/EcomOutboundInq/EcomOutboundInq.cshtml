    <script type="text/javascript">

        var urlBatchPrint = '/EcomOutboundInq/EcomBatchPrint';
        $(function () {
            debugger;
            $('#tblBatchPrint tbody').on('click', '.clsBatchPrint', function () {
                debugger;
                var $buttonClicked = $(this);
                var p_str_cmp_id = $buttonClicked.attr('data-cmp-id');
                var p_str_batch_id = $buttonClicked.attr('data-batch-id');

                var pintTotOrders = $buttonClicked.attr('data-total-order');
                var pintOpenOrders = $buttonClicked.attr('data-open-order');
                var pintAlocOrders = $buttonClicked.attr('data-aloc-order');
                var pintShipOrders = $buttonClicked.attr('data-shipped-order');
                var rec_id = $buttonClicked.attr('data-rec-id');
                var options = { "backdrop": "static", keyboard: true };
                $("#ajaxLoading").show();

                $.ajax({
                    url: urlBatchPrint,
                    dataType: "json",
                    contentType: "application/json",
                    data: JSON.stringify({
                        "p_str_cmp_id": p_str_cmp_id, "p_str_batch_id": p_str_batch_id, "pintTotOrders": pintTotOrders, "pintOpenOrders": pintOpenOrders,
                        "pintAlocOrders": pintAlocOrders, "pintShipOrders": pintShipOrders
                    }),
                    type: "POST",
                    success: function (objEComPrintOrders) {

                        var strPDFFileName = objEComPrintOrders.print_file_name;
                        if (strPDFFileName != '') {
                            $("#batch_status" + rec_id).val(objEComPrintOrders.batch_status);
                            $("#printed_dt" + rec_id).val(objEComPrintOrders.printed_dt);
                            $("#printed_by" + rec_id).val(objEComPrintOrders.printed_by);
                            $("#reprint"+ rec_id).show();
                            alert('batch_status' + rec_id);
                            window.open('ShowPDFReport?strPDFFileName=' + strPDFFileName + '&p_str_cmp_id=' + p_str_cmp_id, 'EcomOutboundInq', "toolbar=no, scrollbars=yes, resizable=yes, top=200, left=500, width=950, height=700", 'EcomOutboundInq' + p_str_batch_id)
                        }
                        else {
                            swal("", "No Orders Allocated/ Documents not found ", "");
                        }
                        $("#ajaxLoading").hide();
                    }
                });
            });

        });
    </script>



