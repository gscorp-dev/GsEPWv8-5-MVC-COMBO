    $('#frm_dt').datetimepicker({
        timepicker: false,
        step: 15, format: 'm-d-Y'
    });
    $('#to_dt').datetimepicker({
        timepicker: false,
        step: 15, format: 'm-d-Y'
    });
    $(function () {
        $('#cmp_id').change(function () {
            debugger;
            var id = $(this).find(":selected").val();
            $("#ajaxLoading").show();
            $.ajax({
                url: '@Url.Action("GetModels", "DashBoard")',
                type: 'GET',
                cache: false,
                data: {
                    "id": id
                },

                dataType: "html",
                success: function (e) {
                    $("#ajaxLoading").hide();
                    $('#ShowDashboard').html("");
                    $('#ShowDashboard').html(e);
                },
            });
        });

    });
    $("#btnRefresh").click(function () {
            debugger;
            var p_str_cmp_Id = $('#cmp_id').val();
            var p_str_frm_dt = $('#frm_dt').val();
            var p_str_to_dt = $('#to_dt').val();
            var options = { "backdrop": "static", keyboard: true };

            $("#ajaxLoading").show();
            $.ajax({
                url: '@Url.Action("DateOnChangeGetModels", "DashBoard")',
