<style>
    .nav ul {
        *zoom: 1;
        list-style: none;
        margin: 0;
        padding: 0;
        background: #fff;
    }



        .nav ul:before, .nav ul:after {
            content: "";
            display: table;
        }

        .nav ul:after {
            clear: both;
        }

        .nav ul > li {
            float: left;
            position: relative;
        }

    .nav a {
        display: block;
        padding: 11px 10px;
        color: #000;
    }

        .nav a:hover {
            text-decoration: none;
            background: #0088cc;
            color: #fff;
        }

    .nav li ul {
        background: #fff;
    }

        .nav li ul li {
            width: 200px;
        }

        .nav li ul a {
            border: none;
        }

            .nav li ul a:hover {
                background: #0088cc;
                color: #fff;
            }

    .nav li ul {
        position: absolute;
        left: 0;
        top: 36px;
        z-index: 1;
        max-height: 0;
        overflow: hidden;
        -webkit-transform: perspective(400) rotate3d(1,0,0,-90deg);
        -webkit-transform-origin: 50% 0;
        -webkit-transition: 350ms;
        -moz-transition: 350ms;
        -o-transition: 350ms;
        transition: 350ms;
    }

    .nav ul > li:hover ul {
        max-height: 1000px;
        -webkit-transform: perspective(400) rotate3d(0,0,0,0);
    }


    .group1 {
        font-family: monospace !important;
    }
</style>
