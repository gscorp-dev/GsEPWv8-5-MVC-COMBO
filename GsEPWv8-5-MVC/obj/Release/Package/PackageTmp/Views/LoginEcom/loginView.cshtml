                    </span>
                  

                  
                    @using (Html.BeginForm("LoginView", "Loginecom", FormMethod.Post, new { @enctype = "multipart/form-data" }))
                    {
                        <div style="padding-top: 20px" class="panel-body">

                            <div style="text-align:center;color:red">@ViewBag.ErrorMessage</div><br />

                            <div style="display: none" id="login-alert" class="alert alert-danger col-sm-12"></div>


                            <div style="margin-bottom: 1px" class="input-group">
                                <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                @Html.TextBoxFor(x => x.Email, new { @class = "form-control", placeholder = "USER NAME", @maxlength = "50", @style = "Background:#eae7e7; height:40px;" })
                                <span style="color:red" id="Emailmsg"></span>
                            </div>
                            <div style="margin-bottom: 25px;color:red;padding-top:1px;"></div>
                            <div style="margin-bottom: 1px" class="input-group">
                                <span class="input-group-addon"><i class="fa fa-lock"></i></span>
                                @Html.TextBoxFor(x => x.Password, new { @class = "form-control", placeholder = "PASSWORD", @maxlength = "10", @type = "PASSWORD", @style = "Background:#eae7e7; height:40px;" })
                                <span style="color:red" id="Pwdmsg"></span>



                            </div>
                            <div style="margin-bottom: 25px;color:red;padding-top:1px;"></div>






                            <div style="margin-top: 10px" class="form-group">
                                <!-- Button -->

                                <div class="col-sm-12 controls">
                                    <input type="submit" name="btnsub" id="btnsub" value="Login" class="btn btn-success" onclick="return LoginAuthenticate();" />

                                    <button id="btn-reset" type="reset" href="#" class="btn btn-reset">Reset</button>
                                </div>
                            </div>


                        </div>
                    }
                </div>
            </div>

        </div>
